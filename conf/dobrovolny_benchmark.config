// Benchmarking configuraiton for dobrovolny method using the 79 meat samples from 'Denay et al. Foods 2023'
params {
	input = "https://raw.githubusercontent.com/bio-raum/FooDMe2/dev/assets/validation/dobrovolny_benchmark_samples.csv"
	primer_set = "16S_ILM_ASU188_meat"
	run_name = "Dobrovolny_benchmark"
    ground_truth = "https://raw.githubusercontent.com/bio-raum/FooDMe2/dev/assets/validation/dobrovolny_benchmark_groundtruth.csv"
    benchmark_rank = "genus"
    benchmark_cutoff = 0.001  // 0.1%
}
